{
  "name": "pequeno",
  "version": "0.1.3",
  "description": "A simpler react static site generator",
  "homepage": "https://github.com/signalkuppe/pequeno",
  "author": {
    "name": "Signalkuppe",
    "url": "https://www.signalkuppe.com"
  },
  "keywords": [
    "react",
    "ssg",
    "static",
    "site",
    "generator",
    "vanilla",
    "js"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "release-it",
    "dev": "node lib/cmd.js  --serve --example --noAfterBuild --noProcessHtml",
    "dev-verbose": "node lib/cmd.js  --verbose --serve --example --clean --noAfterBuild --noProcessHtml",
    "dev-news": "node lib/cmd.js --example --serve --page=news-item --path=/news/news-1-slug/index.html --data=news --noAfterBuild --noProcessHtml",
    "dev-page": "node lib/cmd.js --example --serve --page=test --noAfterBuild --noProcessHtml --noData",
    "dev-empty": "node lib/cmd.js --example --serve --page=empty --noAfterBuild --noProcessHtml --noData --noCopy --noPublicCopy",
    "build": "node lib/cmd.js --example",
    "build-verbose": "node lib/cmd.js --verbose --example --clean"
  },
  "bin": {
    "pequeno": "lib/cmd.js"
  },
  "main": "./lib/exports.js",
  "license": "ISC",
  "dependencies": {
    "async": "3.2.3",
    "browser-sync": "2.27.9",
    "cheerio": "1.0.0-rc.10",
    "chokidar": "3.5.3",
    "esbuild": "0.14.38",
    "esbuild-plugin-svgr": "1.0.0",
    "fast-glob": "3.2.11",
    "fisarmonica": "1.3.5",
    "fs-extra": "10.0.1",
    "lodash": "4.17.21",
    "micromodal": "0.4.10",
    "minimist": "1.2.6",
    "node-cache": "5.1.2",
    "normalize-path": "3.0.0",
    "ora": "5.4.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "sitemap": "7.1.1",
    "styled-components": "5.3.5",
    "vanilla-lazyload": "17.6.1",
    "winston": "3.7.2"
  },
  "devDependencies": {
    "release-it": "14.14.0"
  }
}
